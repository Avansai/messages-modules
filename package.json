{
  "name": "messages-modules",
  "version": "0.0.3",
  "description": "Messages (localized strings) that are scoped locally.",
  "author": "Avansai (https://avansai.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avansai/messages-modules.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./babel-plugin": "./lib/babel-plugin/index.js"
  },
  "typesVersions": {
    "*": {
      "babel-plugin": [
        "lib/babel-plugin/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -Rf ./lib && tsc && npm run lint && npm test",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --fix .",
    "lint-print-config": "eslint --print-config ./eslintrc.yaml",
    "prettier": "prettier --write .",
    "test": "jest --coverage",
    "release": "dotenv -- release-it --only-version"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "keywords": [
    "messages-modules",
    "messages module",
    "messages",
    "MessageFormat",
    "intl",
    "i18n",
    "internationalization"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "release-it": "^15.2.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "properties-file": "^2.0.9"
  }
}
