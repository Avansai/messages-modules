{
  "name": "messages-modules",
  "version": "1.1.7",
  "description": "Messages (localized strings) that are scoped locally.",
  "author": "Avansai (https://avansai.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avansai/messages-modules.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./babel-plugin": "./lib/babel-plugin/index.js"
  },
  "typesVersions": {
    "*": {
      "babel-plugin": [
        "lib/babel-plugin/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "npm run prettier && npm run lint-fix && rm -Rf ./lib && tsc && npm run test",
    "lint-fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --fix .",
    "lint-check": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "lint-print-config": "eslint --print-config ./eslintrc.yaml",
    "prettier": "prettier --write .",
    "test": "jest --coverage",
    "release": "dotenv -- release-it --only-version"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "keywords": [
    "messages-modules",
    "messages module",
    "messages",
    "MessageFormat",
    "intl",
    "i18n",
    "internationalization"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@release-it/conventional-changelog": "^5.1.0",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.21",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-sh": "^0.12.8",
    "release-it": "^15.4.2",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "properties-file": "^2.1.6"
  },
  "engines": {
    "node": "^14.18.1 || ^16.0.0"
  }
}
