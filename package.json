{
  "name": "messages-modules",
  "version": "1.1.31",
  "description": "Messages (localized strings) that are scoped locally.",
  "keywords": [
    "messages-modules",
    "messages module",
    "messages",
    "MessageFormat",
    "intl",
    "i18n",
    "internationalization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avansai/messages-modules.git"
  },
  "license": "MIT",
  "author": "Avansai (https://avansai.com)",
  "exports": {
    ".": "./lib/index.js",
    "./babel-plugin": "./lib/babel-plugin/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "babel-plugin": [
        "lib/babel-plugin/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run prettier && npm run lint-fix && rm -Rf ./lib && tsc && npm run test",
    "lint-check": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json .",
    "lint-fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json --fix .",
    "lint-print-config": "eslint --print-config ./eslintrc.yaml",
    "prettier": "prettier --write .",
    "release": "dotenv -- release-it --only-version",
    "test": "jest --coverage"
  },
  "dependencies": {
    "properties-file": "^2.1.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/babel__core": "^7.20.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json-files": "^2.1.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^45.0.2",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-sh": "^0.12.8",
    "release-it": "^15.6.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "^14.18.1 || >=16.0.0"
  }
}
