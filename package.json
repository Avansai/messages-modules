{
  "name": "messages-modules",
  "version": "1.1.75",
  "description": "Messages (localized strings) that are scoped locally.",
  "keywords": [
    "messages-modules",
    "messages module",
    "messages",
    "MessageFormat",
    "intl",
    "i18n",
    "internationalization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avansai/messages-modules.git"
  },
  "license": "MIT",
  "author": "Avansai (https://avansai.com)",
  "exports": {
    ".": "./lib/index.js",
    "./babel-plugin": "./lib/babel-plugin/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "babel-plugin": [
        "lib/babel-plugin/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run prettier && npm run lint-fix && rm -Rf ./lib && tsc && npm run test",
    "lint-check": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json .",
    "lint-fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json --fix .",
    "lint-print-config": "eslint --print-config ./eslintrc.yaml",
    "prettier": "prettier --write .",
    "release": "dotenv -- release-it --only-version",
    "test": "jest --coverage"
  },
  "dependencies": {
    "properties-file": "3.3.15"
  },
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.23.7",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/babel__core": "7.20.5",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-json-files": "4.1.0",
    "eslint-plugin-prefer-arrow-functions": "3.2.4",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "50.0.1",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-sh": "0.13.1",
    "release-it": "17.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "^14.18.1 || >=16.0.0"
  }
}
