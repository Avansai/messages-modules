{
  "name": "messages-modules",
  "version": "1.2.0",
  "description": "Messages (localized strings) that are scoped locally.",
  "keywords": [
    "messages-modules",
    "messages module",
    "messages",
    "MessageFormat",
    "intl",
    "i18n",
    "internationalization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Avansai/messages-modules.git"
  },
  "license": "MIT",
  "author": "Avansai (https://avansai.com)",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    },
    "./babel-plugin": {
      "import": {
        "types": "./lib/esm/babel-plugin/index.d.ts",
        "default": "./lib/esm/babel-plugin/index.js"
      },
      "require": {
        "types": "./lib/cjs/babel-plugin/index.d.ts",
        "default": "./lib/cjs/babel-plugin/index.js"
      }
    }
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run prettier && npm run lint-fix && rm -Rf ./lib && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json && npm run test",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "release": "dotenv -- release-it --only-version",
    "test": "jest --coverage"
  },
  "dependencies": {
    "properties-file": "3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.24.0",
    "@release-it/conventional-changelog": "8.0.1",
    "@types/babel__core": "7.20.5",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.22",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "dotenv-cli": "7.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-node": "0.3.9",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-json-files": "4.1.0",
    "eslint-plugin-prefer-arrow-functions": "3.3.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "51.0.1",
    "jest": "29.7.0",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-sh": "0.14.0",
    "release-it": "17.1.1",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": "^14.18.1 || >=16.0.0"
  }
}
